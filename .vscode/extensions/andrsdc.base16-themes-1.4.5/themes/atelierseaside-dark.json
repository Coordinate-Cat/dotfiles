{
	"name": "Base16 Dark Atelier Seaside",
	"type": "dark",
	"tokenColors": [
		{
			"settings": {
				"background": "#131513",
				"foreground": "#8ca68c"
			}
		},
		{
			"name": "Text",
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#8ca68c"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#687d68"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.string",
				"punctuation.definition.variable",
				"punctuation.definition.string",
				"punctuation.definition.parameters",
				"punctuation.definition.string",
				"punctuation.definition.array"
			],
			"settings": {
				"foreground": "#8ca68c"
			}
		},
		{
			"name": "Delimiters",
			"scope": "none",
			"settings": {
				"foreground": "#8ca68c"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#8ca68c"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#ad2bee"
			}
		},
		{
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"foreground": "#e6193c"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"meta.require",
				"support.function.any-method"
			],
			"settings": {
				"foreground": "#3d62f5"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"support.class",
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#c3c322"
			}
		},
		{
			"name": "Classes",
			"scope": "meta.class",
			"settings": {
				"foreground": "#f0fff0"
			}
		},
		{
			"name": "Methods",
			"scope": "keyword.other.special-method",
			"settings": {
				"foreground": "#3d62f5"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#ad2bee"
			}
		},
		{
			"name": "Support",
			"scope": "support.function",
			"settings": {
				"foreground": "#1999b3"
			}
		},
		{
			"name": "Strings, Inherited Class",
			"scope": [
				"string",
				"constant.other.symbol",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#29a329"
			}
		},
		{
			"name": "Integers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#87711d"
			}
		},
		{
			"name": "Floats",
			"scope": "none",
			"settings": {
				"foreground": "#87711d"
			}
		},
		{
			"name": "Boolean",
			"scope": "none",
			"settings": {
				"foreground": "#87711d"
			}
		},
		{
			"name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#87711d"
			}
		},
		{
			"name": "Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#e6193c"
			}
		},
		{
			"name": "Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#87711d"
			}
		},
		{
			"name": "Attribute IDs",
			"scope": [
				"entity.other.attribute-name.id",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#3d62f5"
			}
		},
		{
			"name": "Selector",
			"scope": "meta.selector",
			"settings": {
				"foreground": "#ad2bee"
			}
		},
		{
			"name": "Values",
			"scope": "none",
			"settings": {
				"foreground": "#87711d"
			}
		},
		{
			"name": "Headings",
			"scope": [
				"markup.heading punctuation.definition.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#3d62f5"
			}
		},
		{
			"name": "Units",
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#87711d"
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold",
				"punctuation.definition.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#c3c322"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic",
				"punctuation.definition.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ad2bee"
			}
		},
		{
			"name": "Code",
			"scope": "markup.raw.inline",
			"settings": {
				"foreground": "#29a329"
			}
		},
		{
			"name": "Link Text",
			"scope": [
				"string.other.link",
				"punctuation.definition.string.end.markdown"
			],
			"settings": {
				"foreground": "#e6193c"
			}
		},
		{
			"name": "Link Url",
			"scope": "meta.link",
			"settings": {
				"foreground": "#87711d"
			}
		},
		{
			"name": "Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#e6193c"
			}
		},
		{
			"name": "Quotes",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#87711d"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"background": "#5e6e5e",
				"foreground": "#8ca68c"
			}
		},
		{
			"name": "Inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#29a329"
			}
		},
		{
			"name": "Deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#e6193c"
			}
		},
		{
			"name": "Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#ad2bee"
			}
		},
		{
			"name": "Colors",
			"scope": "constant.other.color",
			"settings": {
				"foreground": "#1999b3"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#1999b3"
			}
		},
		{
			"name": "Escape Characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#1999b3"
			}
		},
		{
			"name": "Embedded",
			"scope": [
				"punctuation.section.embedded",
				"variable.interpolation"
			],
			"settings": {
				"foreground": "#e619c3"
			}
		},
		{
			"name": "Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#e6193c",
				"foreground": "#f0fff0"
			}
		},
		{
			"name": "Broken",
			"scope": "invalid.broken",
			"settings": {
				"background": "#87711d",
				"foreground": "#131513"
			}
		},
		{
			"name": "Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#e619c3",
				"foreground": "#f0fff0"
			}
		},
		{
			"name": "Unimplemented",
			"scope": "invalid.unimplemented",
			"settings": {
				"background": "#687d68",
				"foreground": "#f0fff0"
			}
		}
	],
	"colors": {
		// Editor Colors
		"editor.background": "#131513", //primary
		"editorCursor.foreground": "#8ca68c",
		"editor.foreground": "#8ca68c",
		"editorWhitespace.foreground": "#687d68",
		"editor.lineHighlightBackground": "#687d6855",
		"editor.selectionBackground": "#5e6e5e",
		// Workbench: Side Bar
		"sideBar.background": "#111211", // 2nd shade
		"sideBarTitle.foreground": "#8ca68c",
		// Workbench: Activity Bar
		"activityBar.foreground": "#8ca68c",
		"activityBar.background": "#2a2c2a", // 2nd tint
		"activityBarBadge.background": "#717271", //5th tint
		// Workbench: Status Bar
		"statusBar.foreground": "#8ca68c",
		"statusBar.background": "#2a2c2a",
		"statusBar.debuggingBackground": "#717271",
		"statusBar.noFolderBackground": "#717271",
		"statusBarItem.prominentBackground": "#a0a1a0", // 7th tint
		"statusBarItem.prominentHoverBackground": "#a0a1a099",
		// Workbench: Editor Groups
		"editorGroup.background": "#2a2c2a",
		"editorGroup.border": "#8ca68c",
		"editorGroup.dropBackground": "#4a4543",
		"editorGroupHeader.tabsBackground": "#2a2c2a",
		"tab.activeBackground": "#131513",
		"tab.inactiveBackground": "#2a2c2a",
		// Workbench: Notifications
		"notification.background": "#6b6766",
		// Dropdown Control
		"dropdown.background": "#a5a2a2",
		// "dropdown.border": "",
		"dropdown.foreground": "#111211",
		// Button Control
		"button.background": "#4a4543",
		"button.foreground": "#8ca68c",
		// Lists and Trees
		"list.activeSelectionBackground": "#2a2c2a",
		"list.dropBackground": "#2a2c2a",
		"list.focusBackground": "#2a2c2a",
		"list.hoverBackground": "#2a2c2a",
		"list.inactiveSelectionBackground": "#2a2c2a"
	}
}