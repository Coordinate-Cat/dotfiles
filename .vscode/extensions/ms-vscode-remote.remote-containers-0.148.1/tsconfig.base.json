{
	"compilerOptions": {
		"composite": true,
		"rootDir": ".",
		"outDir": "./built",
		"target": "es2018",
		"module": "commonjs",
		"strict": true,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"newLine": "LF"
	}
}