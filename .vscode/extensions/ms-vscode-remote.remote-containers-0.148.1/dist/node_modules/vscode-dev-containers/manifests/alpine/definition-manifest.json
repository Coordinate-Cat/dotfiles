{
	"variants": ["3.12", "3.11", "3.10"],
	"build": {
		"rootDistro": "alpine",
		"tags": [
			"base:${VERSION}-alpine-${VARIANT}",
			"base:${VERSION}-alpine${VARIANT}",
			"base:${VERSION}-alpine"
		]
	},
	"dependencies": {
		"image": "alpine:${VARIANT}",
		"imageLink": "https://hub.docker.com/_/alpine",
		"alpine": [
			"git",
			"bash",
			"libgcc",
			"libstdc++",
			"curl",
			"openssh-client",
			"less",
			"wget",
			"nano",
			"jq",
			"gnupg",
			"unzip",
			"procps",
			"coreutils",
			"ca-certificates",
			"krb5-libs",
			"libintl",
			"libssl1.1",
			"lttng-ust",
			"tzdata",
			"userspace-rcu",
			"zlib",
			"shadow",
			"sudo",
			"zsh"
		],
		"git": {
			"Oh My Zsh!": "/root/.oh-my-zsh"
		}
	}
}