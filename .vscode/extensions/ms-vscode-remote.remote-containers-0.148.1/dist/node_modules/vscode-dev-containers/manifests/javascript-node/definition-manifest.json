{
	"variants": ["14-buster", "12-buster", "10-buster","14-stretch", "12-stretch", "10-stretch"],
	"build": {
		"latest": true,
		"rootDistro": "debian",
		"tags": [
			"javascript-node:${VERSION}-${VARIANT}"
		],
		"variantTags": {
			"14-buster": [ "javascript-node:${VERSION}-14" ],
			"12-buster": [ "javascript-node:${VERSION}-12" ],
			"10-buster": [ "javascript-node:${VERSION}-10" ]
		}
	}, 
	"dependencies": {
		"image": "node:${VARIANT}",
		"imageLink": "https://hub.docker.com/_/node",
		"debian": [
			"apt-utils",
			"git",
			"openssh-client",
			"gnupg2",
			"iproute2",
			"procps",
			"lsof",
			"htop",
			"net-tools",
			"psmisc",
			"curl",
			"wget",
			"rsync",
			"ca-certificates",
			"unzip",
			"zip",
			"tar",
			"nano",
			"vim-tiny",
			"less",
			"jq",
			"lsb-release",
			"apt-transport-https",
			"dialog",
			"libc6",
			"libgcc1",
			"libgssapi-krb5-2",
			"libicu[0-9][0-9]",
			"liblttng-ust0",
			"libstdc++6",
			"zlib1g",
			"locales",
			"sudo",
			"ncdu",
			"man-db",
			"zsh",
			"yarn"
		],
		"git": {
			"Oh My Zsh!": "/root/.oh-my-zsh",
			"nvm": "/usr/local/share/nvm"
		},
		"npm": [
			"eslint"
		]
	}
}