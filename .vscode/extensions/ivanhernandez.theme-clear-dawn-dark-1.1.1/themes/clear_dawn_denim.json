    "tokenColors": [{
        "scope": "emphasis",
        "settings": {
            "fontStyle": "italic"
        }
    },
    {
        "scope": "strong",
        "settings": {
            "fontStyle": "bold"
        }
    },
    {
        "scope": "meta.diff.header",
        "settings": {
            "foreground": "#C0C0CE"
        }
    },
    {
        "scope": "comment",
        "settings": {
            "foreground": "#6171A1"
        }
    },
    {
        "scope": "constant.language",
        "settings": {
            "foreground": "#BCF"
        }
    },
    {
        "scope": [
            "constant.numeric"
        ],
        "settings": {
            "foreground": "#FBC"
        }
    },
    {
        "scope": "constant.regexp",
        "settings": {
            "foreground": "#FF99AA"
        }
    },
    {
        "name": "css tags in selectors, xml tags",
        "scope": "entity.name.tag",
        "settings": {
            "fontStyle": "bold"
        }
    },
    {
        "scope": "entity.name.selector",
        "settings": {
            "foreground": "#C0C0CE" //rgb(16, 8, 0)
        }
    },
    {
        "scope": "entity.other.attribute-name",
        "settings": {
            "foreground": "#C0C0CE" //rgb(16, 8, 0)
        }
    },
    {
        "scope": [
            "entity.other.attribute-name.class.css",
            "entity.other.attribute-name.class.mixin.css",
            "entity.other.attribute-name.parent-selector.css",
            "entity.other.attribute-name.pseudo-class.css",
            "entity.other.attribute-name.pseudo-element.css",
            "entity.other.attribute-name.attribute.scss",
            "entity.other.attribute-name.scss"
        ],
        "settings": {
            "foreground": "#9AE" //rgb(97, 113, 161)
        }
    },
    {
        "scope": [
            "entity.other.attribute-name.id.css",
            "source.css.less entity.other.attribute-name.id"
        ],
        "settings": {
            "foreground": "#A16191" //rgb(161, 97, 145)
        }
    },
    {
        "scope": "invalid",
        "settings": {
            "foreground": "#F50" //rgb(16, 8, 0)
        }
    },
    {
        "scope": "markup.underline",
        "settings": {
            "fontStyle": "underline"
        }
    },
    {
        "scope": "markup.bold",
        "settings": {
            "fontStyle": "bold"
        }
    },
    {
        "scope": "markup.heading",
        "settings": {
            "fontStyle": "bold",
            "foreground": "#ADF" //rgb(97, 113, 161)
        }
    },
    {
        "scope": "markup.italic",
        "settings": {
            "fontStyle": "italic"
        }
    },
    {
        "scope": "markup.list",
        "settings": {
            "foreground": "#C7A"
        }
    },
    {
        "scope": "markup.list.numbered",
        "settings": {
            "foreground": "#C7A"
        }
    },
    {
        "scope": "markup.list.numbered",
        "settings": {
            "foreground": "#C7A"
        }
    },
    {
        "scope": "markup.quote",
        "settings": {
            "foreground": "#2DB"
        }
    },
    {
        "scope": "markup.raw",
        "settings": {
            "foreground": "#2DB"
        }
    },
    {
        "scope": "markup.inline.raw",
        "settings": {
            "foreground": "#2DB"
        }
    },
    {
        "scope": "markup.inserted",
        "settings": {
            "foreground": "#2DB"
        }
    },
    {
        "scope": "markup.deleted",
        "settings": {
            "foreground": "#E56"
        }
    },
    {
        "scope": "markup.changed",
        "settings": {
            "foreground": "#7BE"
        }
    },
    {
        "scope": [
            "beginning.punctuation.definition.quote.markdown",
            "beginning.punctuation.definition.list.markdown"
        ],
        "settings": {
            "foreground": "#BFF"
        }
    },
    {
        "scope": "markup.inline.raw",
        "settings": {
            "foreground": "#BFF"
        }
    },
    {
        "scope": "meta.selector",
        "settings": {
            "foreground": "#BFF"
        }
    },
    {
        "name": "brackets of XML/HTML tags",
        "scope": "punctuation.definition.tag",
        "settings": {
            "foreground": "#88A"
        }
    },
    {
        "scope": "meta.preprocessor",
        "settings": {
            "foreground": "#C0C0CE"
        }
    },
    {
        "scope": "meta.preprocessor.string",
        "settings": {
            "foreground": "#FBC"
        }
    },
    {
        "scope": "meta.preprocessor.numeric",
        "settings": {
            "foreground": "#BFF"
        }
    },
    {
        "scope": "meta.structure.dictionary.key.python",
        "settings": {
            "foreground": "#C0C0CE" //rgb(16, 8, 0)
        }
    },
    {
        "scope": "storage",
        "settings": {
            "fontStyle": "bold"
        }
    },
    {
        "scope": "storage.type",
        "settings": {
            "fontStyle": "bold"
        }
    },
    {
        "scope": "storage.modifier",
        "settings": {
            "fontStyle": "bold",
        }
    },
    {
        "scope": "string",
        "settings": {
            "foreground": "#FBC"
        }
    },
    {
        "scope": [
            "string.comment.buffered.block.jade",
            "string.quoted.jade",
            "string.interpolated.jade",
            "string.unquoted.plain.in.yaml",
            "string.unquoted.plain.out.yaml",
            "string.unquoted.block.yaml",
            "string.quoted.single.yaml",
            "string.quoted.double.xml",
            "string.quoted.single.xml",
            "string.unquoted.cdata.xml",
            "string.quoted.double.html",
            "string.quoted.single.html",
            "string.unquoted.html",
            "string.quoted.single.handlebars",
            "string.quoted.double.handlebars"
        ],
        "settings": {
            "foreground": "#FBC" //rgb(161, 49, 49)
        }
    },
    {
        "scope": "string.regexp",
        "settings": {
            "foreground": "#FF99AA"
        }
    },
    {
        "name": "JavaScript string interpolation ${}",
        "scope": [
            "punctuation.definition.template-expression.begin.js",
            "punctuation.definition.template-expression.begin.ts",
            "punctuation.definition.template-expression.end.ts",
            "punctuation.definition.template-expression.end.js"
        ],
        "settings": {
            "foreground": "#DAE"
        }
    },
    {
        "scope": [
            "support.constant.property-value",
            "support.constant.font-name",
            "support.constant.media-type",
            "support.constant.media",
            "constant.other.color.rgb-value",
            "constant.other.rgb-value",
            "support.constant.color"
        ],
        "settings": {}
    },
    {
        "scope": [
            "support.type.vendored.property-name",
            "support.type.property-name",
            "variable.css",
            "variable.scss",
            "variable.other.less"
        ],
        "settings": {}
    },
    {
        "scope": "support.type.property-name.json",
        "settings": {
            "foreground": "#BCF"
        }
    },
    {
        "scope": [
            "punctuation.section.embedded.begin.php",
            "punctuation.section.embedded.end.php"
        ],
        "settings": {}
    },
    {
        "scope": "support.function.git-rebase",
        "settings": {
            "foreground": "#0080F0" //rgb(0, 128, 240)
        }
    },
    {
        "scope": "constant.sha.git-rebase",
        "settings": {
            "foreground": "#00C080" //rgb(0, 192, 128)
        }
    },
    {
        "name": "coloring of the Java import and package identifiers",
        "scope": [
            "storage.modifier.import.java",
            "variable.language.wildcard.java",
            "storage.modifier.package.java"
        ],
        "settings": {
            "fontStyle": "italic"
        }
    },
    {
        "name": "this.self",
        "scope": "variable.language",
        "settings": {
            "foreground": "#519171" //rgb(81, 145, 113)
        }
    },
    {
        "name": "Keywords",
        "scope": [
            "keyword",
            "keyword.control"
        ],
        "settings": {
            "fontStyle": "bold"
        }
    },
    {
        "name": "Operators",
        "scope": [
            "keyword.operator"
        ],
        "settings": {
            "foreground": "#597"
        }
    },
    {
        "scope": [
            "keyword.operator.new",
            "keyword.operator.expression",
            "keyword.operator.cast",
            "keyword.operator.sizeof",
            "keyword.operator.logical.python"
        ],
        "settings": {
            "foreground": "#519171" //rgb(81, 145, 113)
        }
    },
    {
        "scope": "keyword.other.unit",
        "settings": {
            "foreground": "#519171" //rgb(81, 145, 113)
        }
    },
    {
        "name": "Function declarations",
        "scope": [
            "entity.name.function",
            "support.function",
            "support.constant.handlebars"
        ],
        "settings": {
            "foreground": "#D0D0FE" //rgb(97, 113, 161)
        }
    },
    {
        "name": "Types declaration and references",
        "scope": [
            "meta.return-type",
            "support.class",
            "support.type",
            "entity.name.type",
            "entity.name.class",
            "storage.type.cs",
            "storage.type.generic.cs",
            "storage.type.modifier.cs",
            "storage.type.variable.cs",
            "storage.type.annotation.java",
            "storage.type.generic.java",
            "storage.type.java",
            "storage.type.object.array.java",
            "storage.type.primitive.array.java",
            "storage.type.primitive.java",
            "storage.type.token.java",
            "storage.type.groovy",
            "storage.type.annotation.groovy",
            "storage.type.parameters.groovy",
            "storage.type.generic.groovy",
            "storage.type.object.array.groovy",
            "storage.type.primitive.array.groovy",
            "storage.type.primitive.groovy"
        ],
        "settings": {
            "fontStyle": "bold"
        }
    },
    {
        "name": "Types declaration and references, TS grammar specific",
        "scope": [
            "meta.type.cast.expr",
            "meta.type.new.expr",
            "support.constant.math",
            "support.constant.dom",
            "support.constant.json"
        ],
        "settings": {
            "foreground": "#A16191" //rgb(161, 97, 145)
        }
    },
    {
        "name": "Inherited class, TS grammar specific",
        "scope": [
            "entity.other.inherited-class"
        ],
        "settings": {
            "fontStyle": "bold"
        }
    },
    {
        "name": "Variable and parameter name",
        "scope": [
            "variable",
            "meta.definition.variable.name",
            "support.variable"
        ],
        "settings": {}
    },
    {
        "name": "Object keys, TS grammar specific",
        "scope": [
            "meta.object-literal.key",
            "meta.object-literal.key entity.name.function"
        ],
        "settings": {
            "fontStyle": "italic"
        }
    },
    {
        "name": "CSS property value",
        "scope": [
            "support.constant.property-value",
            "support.constant.font-name",
            "support.constant.media-type",
            "support.constant.media",
            "constant.other.color.rgb-value",
            "constant.other.rgb-value",
            "support.constant.color"
        ],
        "settings": {
            "foreground": "#9AE" //rgb(97, 113, 161)
        }
    }
]
}