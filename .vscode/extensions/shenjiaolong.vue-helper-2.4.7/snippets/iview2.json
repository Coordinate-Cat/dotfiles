  "iv-columns": {
    "prefix": "iv-columns",
    "body": [
      "{",
      "\ttitle: $1,",
      "\tkey: $2",
      "}"
    ],
    "description": "columns"
  },
  "iv-columns-center": {
    "prefix": "iv-columns",
    "body": [
      "{",
      "\ttitle: $1,",
      "\tkey: $2,",
      "\talign: 'center'",
      "}"
    ],
    "description": "columns"
  },
  "iv-columns-render": {
    "prefix": "iv-columns-render",
    "body": [
      "{",
      "\ttitle: $1,",
      "\tkey: $2,",
      "\trender: (h, params) => {",
      "\t\t$3",
      "\t}",
      "}"
    ],
    "description": "columns"
  },
  "iv-form-validate-clear": {
    "prefix":"iv-form-validate-clear",
    "body":[
      "this.\\$refs['${1:form}'].resetFields()"
    ]
  },
  "iv-form-validate": {
    "prefix":"iv-form-validate",
    "body":[
      "this.\\$refs['${1:form}'].validate((valid) => {",
      "\tif (valid) {",
      "\t\t$2",
      "\t}",
      "})"
    ]
  },
  "iv-rules": {
    "prefix": "iv-rules",
    "body": [
      "{required: false, trigger: 'blur', validator: validateLength}"
    ]
  },
  "iv-rules-method": {
    "prefix": "iv-rules-method",
    "body": [
      "const validateLength = (rule, value, callback) => {",
      "\tif (value && value.length > 128) {",
      "\t\tcallback(new Error('最大长度不能大于128位'))",
      "\t} else {",
      "\t\tcallback()",
      "\t}",
      "}"
    ]
  },
