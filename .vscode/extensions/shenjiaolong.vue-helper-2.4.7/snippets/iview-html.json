{
  "iv-form-input": {
    "prefix": "iv-form-input",
    "body": [
      "<FormItem label=\"$1\">",
      "\t<Input v-model=\"$2\" placeholder=\"$3\"></Input>",
      "</FormItem>"
    ]
  },
  "iv-form-select": {
    "prefix": "iv-form-select",
    "body": [
      "<FormItem label=\"$1\">",
      "\t<Select v-model=\"$2\">",
      "\t\t<Option value=\"$3\">$4</Option>",
      "\t</Select>",
      "</FormItem>"
    ]
  },
  "iv-form-radio": {
    "prefix": "iv-form-radio",
    "body":[
      "<FormItem label=\"$1\">",
      "\t<RadioGroup v-model=\"$2\">",
      "\t\t<Radio label=\"$3\">$4</Radio>",
      "\t</RadioGroup>",
      "</FormItem>"
    ]
  },
  "iv-form-checkbox": {
    "prefix": "iv-form-checkbox",
    "body":[
      "<FormItem label=\"$1\">",
      "\t<CheckboxGroup v-model=\"$2\">",
      "\t\t<Checkbox label=\"$3\">$4</Checkbox>",
      "\t</CheckboxGroup>",
      "</FormItem>"
    ]
  },
  "iv-form-textarea": {
    "prefix": "iv-form-textarea",
    "body": [
      "<FormItem label=\"$1\">",
      "\t<Input type=\"textarea\" v-model=\"$2\" placeholder=\"$3\"></Input>",
      "</FormItem>"
    ]
  },
  "iv-form": {
    "prefix": "iv-form",
    "body":[
      "<Form :model=\"$1\" :label-width=\"${2:80}\">",
      "\t$3",
      "</Form>"
    ]
  },
  "iv-menu-sub": {
    "prefix": "iv-menu-sub",
    "body": [
      "<Menu active-name=\"1-1\">",
      "\t<Submenu name=\"1\">",
      "\t\t<template slot=\"title\">",
      "\t\t\t<Icon type=\"ios-people\" /> 用户管理",
      "\t\t</template>",
      "\t\t<MenuItem name=\"1-1\">新增用户</MenuItem>",
      "\t\t<MenuItem name=\"1-2\">活跃用户</MenuItem>",
      "\t</Submenu>",
      "</Menu>"
    ]
  },
  "iv-menu": {
    "prefix": "iv-menu",
    "body": [
      "<Menu active-name=\"1-1\">",
      "\t<MenuItem name=\"1-1\">新增用户</MenuItem>",
      "</Menu>"
    ]
  },
  "iv-dropdown": {
    "prefix": "iv-dropdown",
    "body": [
      "<Dropdown>",
      "\t<Button type=\"primary\">",
      "\t\t下拉菜单",
      "\t\t<Icon type=\"ios-arrow-down\"></Icon>",
      "\t</Button>",
      "\t<DropdownMenu slot=\"list\">",
      "\t\t<DropdownItem>驴打滚</DropdownItem>",
      "\t</DropdownMenu>",
      "</Dropdown>"
    ]
  },
  "iv-page": {
    "prefix": "iv-page",
    "body": [
      "<Page :total=\"pageTotal\"",
      ":current.sync=\"currentPage\"",
      ":show-total=\"true\"",
      ":page-size=\"pageSize\"",
      ":show-sizer=\"true\"",
      ":show-elevator=\"true\"",
      ":page-size-opts=\"pageSizeList\"",
      "@on-change=\"pageChange\"",
      "@on-page-size-change=\"pageSizeChange\" >",
      "</Page>"
    ]
  }
}